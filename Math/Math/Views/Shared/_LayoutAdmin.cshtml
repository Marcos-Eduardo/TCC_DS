    <meta name="description" content="">

    <title>@ViewData["Title"]</title>

    <script src="https://kit.fontawesome.com/64d58efce2.js" crossorigin="anonymous"></script>

    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />

    <link rel="stylesheet" href="~/fe/assets/web/assets/mobirise-icons/mobirise-icons.css">
    <link rel="stylesheet" href="~/fe/assets/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/fe/assets/bootstrap/css/bootstrap-grid.min.css">
    <link rel="stylesheet" href="~/fe/assets/bootstrap/css/bootstrap-reboot.min.css">
    <link rel="stylesheet" href="~/fe/assets/tether/tether.min.css">
    <link rel="stylesheet" href="~/fe/assets/dropdown/css/style.css">
    <link rel="stylesheet" href="~/fe/assets/socicon/css/styles.css">
    <link rel="stylesheet" href="~/fe/assets/theme/css/style.css">
    <link rel="preload" as="style" href="~/fe/assets/mobirise/css/mbr-additional.css">
    <link rel="stylesheet" href="~/fe/assets/mobirise/css/mbr-additional.css" type="text/css">
</head>
<body>
    <section class="menu cid-rI1bzieexa" once="menu" id="menu1-0">
        <nav class="navbar navbar-expand beta-menu navbar-dropdown align-items-center navbar-fixed-top navbar-toggleable-sm">
            <button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <div class="hamburger">
                    <span></span>
                    <span></span>
                    <span></span>
                    <span></span>
                </div>
            </button>
            <div class="menu-logo">
                <div class="navbar-brand">
                    <span class="navbar-logo">
                        <a href="#top">
                            <img src="~/fe/assets/images/logo.svg" alt="Logo" title="" style="height: 3.8rem;">
                        </a>
                    </span>
                </div>
            </div>
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav nav-dropdown nav-right" data-app-modern-menu="true">
                    <li class="nav-item">
                        <a class="nav-link link text-white display-4" href="#">
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link link text-white display-4" asp-controller="Admin" asp-action="Index">
                            <span class="mbri-home mbr-iconfont mbr-iconfont-btn"></span>
                            Home
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link link text-white display-4" asp-controller="Conteudos" asp-action="Index">
                            <span class="mbri-edit mbr-iconfont mbr-iconfont-btn"></span>

                            Conteúdos
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link link text-white display-4" asp-controller="Niveis" asp-action="Index">
                            <span class="mbri-growing-chart mbr-iconfont mbr-iconfont-btn"></span>

                            Níveis
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link link text-white display-4" asp-controller="Dicas" asp-action="Index">
                            <span class="mbri-video mbr-iconfont mbr-iconfont-btn"></span>

                            Dicas
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link link text-white display-4" asp-controller="Quizes" asp-action="Index">
                            <span class="mbri-setting3 mbr-iconfont mbr-iconfont-btn"></span>

                            Quizzes
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link link text-white display-4" asp-controller="Questoes" asp-action="Index">
                            <span class="mbri-question mbr-iconfont mbr-iconfont-btn"></span>

                            Questões
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link link text-white display-4" asp-controller="Account" asp-action="Index">
                            <span class="mbri-users mbr-iconfont mbr-iconfont-btn"></span>

                            Usuários
                        </a>
                    </li>
                </ul>
                <form asp-controller="Account" asp-action="Logout">
                    <div class="navbar-buttons mbr-section-btn">
                        <button type="submit" class="btn btn-sm btn-white-outline display-4 rounded-pill">
                            <span class="mbri-logout mbr-iconfont mbr-iconfont-btn"></span>

                            Sair
                        </button>
                    </div>
                </form>
            </div>
        </nav>
    </section>
    <div style="@(ViewData["Title"].ToString() != "Home" ? "top: 100px;" : "")" class="@(ViewData["Title"].ToString() != "Home" ? "container" : "")">
        <main role="main">
            @RenderBody()
        </main>
    </div>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    @*<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>*@
    <script src="~/js/site.js" asp-append-version="true">
        </script>

        <script src="~/fe/assets/web/assets/jquery/jquery.min.js"></script>
        <script src="~/fe/assets/popper/popper.min.js"></script>
        <script src="~/fe/assets/bootstrap/js/bootstrap.min.js"></script>
        <script src="~/fe/assets/tether/tether.min.js"></script>
        <script src="~/fe/assets/smoothscroll/smooth-scroll.js"></script>
        <script src="~/fe/assets/dropdown/js/nav-dropdown.js"></script>
        <script src="~/fe/assets/dropdown/js/navbar-dropdown.js"></script>
        <script src="~/fe/assets/touchswipe/jquery.touch-swipe.min.js"></script>
        <script src="~/fe/assets/theme/js/script.js"></script>
